generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id      String  @id @default(uuid())
  title   String 
  details String?
  maximumAttendees Int? @map("maximum_attendees")
  slug    String  @unique


  @@map("events")
}
